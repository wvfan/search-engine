{
  "name": "app",
  "version": "1.0.0",
  "private": false,
  "engines": {
    "node": "6.11.1",
    "npm": ">=3.8"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "ejs": "^2.5.9",
    "express": "^4.16.3",
    "lodash": "^4.17.10",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "auto-bind": "^1.2.0",
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.24.0",
    "browser-sync": "^2.18.8",
    "chai": "^4.0.0-canary.1",
    "classnames": "^2.2.5",
    "connect-history-api-fallback": "^1.3.0",
    "css-loader": "^0.27.3",
    "dateformat": "^3.0.2",
    "echarts": "^3.7.2",
    "ejs": "^2.5.9",
    "elasticsearch": "^14.2.2",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.2",
    "fastclick": "^1.0.6",
    "fb": "^2.0.0",
    "fetch-polyfill": "^0.8.2",
    "file-loader": "^0.10.1",
    "front-matter": "^2.1.2",
    "highlight.js": "^9.10.0",
    "history": "^4.6.1",
    "json-loader": "^0.5.4",
    "localStorage": "^1.0.3",
    "lodash": "^4.17.10",
    "markdown-it": "^8.3.1",
    "mobx": "^3.4.1",
    "mobx-react": "^4.3.5",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.3",
    "path-to-regexp": "^1.7.0",
    "pixrem": "^3.0.2",
    "pleeease-filters": "^3.0.1",
    "prop-types": "^15.5.10",
    "qr-image": "^3.2.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.2",
    "react-mdl": "^1.9.0",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha",
    "react-transition-group": "1.x",
    "redux-localstorage": "^0.4.1",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^4.3.0",
    "redux-thunk": "^2.2.0",
    "request": "^2.81.0",
    "rimraf": "^2.6.1",
    "s3": "^4.4.0",
    "sass-loader": "^6.0.6",
    "scss-to-json": "^2.0.0",
    "style-loader": "^0.14.1",
    "stylelint": "^7.9.0",
    "stylelint-config-standard": "^16.0.0",
    "tape": "^4.7.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.7.0",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1",
    "whatwg-fetch": "^2.0.3"
  },
  "babel": {
    "presets": [
      "latest",
      "stage-1",
      "react"
    ],
    "plugins": [
      "transform-runtime",
      "transform-decorators-legacy",
      "transform-function-bind"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
      "react/jsx-filename-extension": "off",
      "react/jsx-no-bind": "off",
      "jsx-a11y/img-has-alt": "off",
      "jsx-a11y/no-static-element-interactions": "off",
      "react/require-default-props": "off",
      "import/no-extraneous-dependencies": "off",
      "import/no-named-as-default-member": "off",
      "import/no-duplicates": "off",
      "import/no-named-as-default": "off",
      "import/no-absolute-path": "off",
      "react/prefer-stateless-function": "off",
      "react/forbid-prop-types": "off",
      "react/no-unused-prop-types": "off",
      "no-console": "off",
      "no-unused-vars": "off",
      "no-use-before-define": "off",
      "no-param-reassign": "off",
      "no-fallthrough": "off",
      "no-shadow": "off",
      "no-underscore-dangle": "off",
      "no-mixed-operators": "off",
      "max-len": "off",
      "consistent-return": "off",
      "no-nested-ternary": "off",
      "no-template-curly-in-string": "off",
      "no-eval": "off",
      "no-else-return": "off",
      "prefer-template": "off",
      "no-lonely-if": "off",
      "arrow-body-style": "off",
      "no-continue": "off",
      "react/no-multi-comp": "off",
      "no-loop-func": "off"
    },
    "env": {
      "browser": true
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "tools/webpack.config.js"
        }
      }
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "string-quotes": "single"
    }
  },
  "scripts": {
    "eslint": "eslint components src test tools postcss.config.js webpack.config.js",
    "stylelint": "stylelint \"components/**/*.css\" \"src/**/*.css\"",
    "lint": "npm run eslint && npm run stylelint",
    "test": "mocha --compilers js:babel-register",
    "test:watch": "mocha --compilers js:babel-register --reporter min --watch",
    "build": "node tools/build.js",
    "build:debug": "node tools/build.js --debug",
    "publish": "node tools/publish.js",
    "publish:debug": "node tools/publish.js --debug",
    "start": "node tools/run.js",
    "cordova:android": "node tools/build.js --android && cordova run android",
    "cordova:android:build": "node tools/build.js --android && cordova build android",
    "cordova:ios": "node tools/build.js --ios && cordova run ios",
    "cordova:ios:build": "node tools/build.js --ios && cordova build ios",
    "deploy": "babel cloud/*.js cloud/*/*.js -d cloud_build && node tools/build.js --release && heroku git:remote -a plutus-music && git add . && git commit -am \"make it better\" && git push heroku master",
    "deploy:backend": "babel cloud/*.js cloud/*/*.js -d cloud_build && heroku git:remote -a plutus-music && git add . && git commit -am \"make it better\" && git push heroku master"
  },
  "main": "index.js"
}
